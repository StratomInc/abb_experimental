<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
<xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro"/>

  <xacro:macro name="abb_irb4600_40_255" params="prefix">
    <!-- link list -->
    <link name="${prefix}base_link">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_common/collision/base_link.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_common/visual/base_link.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow">
          <color rgba="1 1 0 1"/>
        </material>
      </visual>
    </link>
    <link name="${prefix}link_1">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_common/collision/link_1.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_common/visual/link_1.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_2">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_2.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_2.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_3">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_3.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_3.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_4">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_4.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_4.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_5">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_5.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_5.stl"/>
        </geometry>
        <xacro:material_abb_white/>
        <material name="yellow"/>
      </visual>
    </link>
    <link name="${prefix}link_6">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.03" iyy="0.03" izz="0.03" ixy="0.0" ixz="0.0" iyz="0.0" />
      </inertial>
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_6.stl"/>
        </geometry>
        <xacro:material_abb_white/>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_6.stl"/>
        </geometry>
